<script>
	function dlcount(count,vtxt,text){
	var count= count;
	var text= text;
	var vtxt= vtxt;
    var countdownElement = document.getElementById(count),
       
        seconds = 10,
        second = 0,
        interval;
   
    interval = setInterval(function() {
       // countdownElement.firstChild.data = "You can start your download in " + (seconds - second) + " seconds";
		countdownElement.innerText=(vtxt+" " + (seconds - second) + " seconds");
        if (second >= seconds) {
           countdownElement.innerText=(text);
            
            clearInterval(interval);
        }
        second++;
    }, 1000);
}
</script>
